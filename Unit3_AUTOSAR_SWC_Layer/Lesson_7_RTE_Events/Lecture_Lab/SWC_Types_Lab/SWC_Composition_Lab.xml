<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://autosar.org/schema/r4.0 file/../../AUTOSAR_ARXML_Schema/AUTOSAR_00046.xsd">
	<AR-PACKAGES>
		<AR-PACKAGE>
			<SHORT-NAME>SWC_Composition</SHORT-NAME>
			<!-- Learn-in-depth Eng.Mina Unit3 SWCs, Session Composition and connectors -->
			<ELEMENTS>
				<!-- Interfaces -->
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SRI_LightControl</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>Light_Control</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SRI_LightActuate</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>LightActuate</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">/AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				<!-- SWCs Defination -->
				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
					<SHORT-NAME>LightControlStatus_SWC</SHORT-NAME>
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>Pp_SR_LightControl</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/SRI_LightControl</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
					</PORTS>
				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
				<APPLICATION-SW-COMPONENT-TYPE>
					<SHORT-NAME>Algo_SWC</SHORT-NAME>
					<PORTS>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>Rp_SR_LightControl</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/SRI_LightControl</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>Pp_SR_LightActuate</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/SRI_LightActuate</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
					</PORTS>
				</APPLICATION-SW-COMPONENT-TYPE>
				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
					<SHORT-NAME>LightActuate_SWC</SHORT-NAME>
					<PORTS>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>Rp_SR_LightActuate</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/SRI_LightActuate</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>
					</PORTS>
				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>
				<!-- Composite SWCs -->
				<COMPOSITION-SW-COMPONENT-TYPE>
					<SHORT-NAME>Compsite1_SWC</SHORT-NAME>
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>Pp_SR_LightActuate_Composite</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF
								DEST="SENDER-RECEIVER-INTERFACE">/SRI_LightActuate</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
					</PORTS>
					<COMPONENTS>
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>LightControl_SA_SWC1</SHORT-NAME>
							<TYPE-TREF DEST="SENSOR-ACTUATOR-SW-COMPONENT-TYPE">/LightControlStatus_SWC</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>LA_SWC2</SHORT-NAME>
							<TYPE-TREF DEST="APPLICATION-SW-COMPONENT-TYPE">/Algo_SWC</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
					</COMPONENTS>
					<CONNECTORS>
						<ASSEMBLY-SW-CONNECTOR>
							<SHORT-NAME>AS_LightControl</SHORT-NAME>
							<PROVIDER-IREF>
								<CONTEXT-COMPONENT-REF
									DEST="SW-COMPONENT-PROTOTYPE">/Compsite1_SWC/LightControl_SA_SWC1</CONTEXT-COMPONENT-REF>
								<TARGET-P-PORT-REF DEST="P-PORT-PROTOTYPE">/LightControlStatus_SWC/Pp_SR_LightControl</TARGET-P-PORT-REF>
							</PROVIDER-IREF>
							<REQUESTER-IREF>
								<CONTEXT-COMPONENT-REF
									DEST="SW-COMPONENT-PROTOTYPE">/Compsite1_SWC/LA_SWC2</CONTEXT-COMPONENT-REF>
								<TARGET-R-PORT-REF DEST="R-PORT-PROTOTYPE">/Algo_SWC/Rp_SR_LightControl</TARGET-R-PORT-REF>
							</REQUESTER-IREF>
						</ASSEMBLY-SW-CONNECTOR>
						<DELEGATION-SW-CONNECTOR>
							<SHORT-NAME>De_LightActuate</SHORT-NAME>
							<INNER-PORT-IREF>
								<P-PORT-IN-COMPOSITION-INSTANCE-REF>
									<CONTEXT-COMPONENT-REF
										DEST="SW-COMPONENT-PROTOTYPE">/Compsite1_SWC/LA_SWC2</CONTEXT-COMPONENT-REF>
									<TARGET-P-PORT-REF DEST="P-PORT-PROTOTYPE">/Algo_SWC/Pp_SR_LightActuate</TARGET-P-PORT-REF>
								</P-PORT-IN-COMPOSITION-INSTANCE-REF>
							</INNER-PORT-IREF>
							<OUTER-PORT-REF DEST="P-PORT-PROTOTYPE">/Compsite1_SWC/Pp_SR_LightActuate_Composite</OUTER-PORT-REF>
						</DELEGATION-SW-CONNECTOR>
					</CONNECTORS>
				</COMPOSITION-SW-COMPONENT-TYPE>
				<COMPOSITION-SW-COMPONENT-TYPE>
					<SHORT-NAME>TopLevel_Composite_SWC</SHORT-NAME>
					<COMPONENTS>
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>Composite1_SWC_Inst</SHORT-NAME>
							<TYPE-TREF DEST="COMPOSITION-SW-COMPONENT-TYPE">/Compsite1_SWC</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
						<SW-COMPONENT-PROTOTYPE>
							<SHORT-NAME>LightActuate_SWC3</SHORT-NAME>
							<TYPE-TREF DEST="SENSOR-ACTUATOR-SW-COMPONENT-TYPE">/LightActuate_SWC</TYPE-TREF>
						</SW-COMPONENT-PROTOTYPE>
					</COMPONENTS>
					<CONNECTORS>
						<ASSEMBLY-SW-CONNECTOR>
							<SHORT-NAME>Assembly_LightActuate</SHORT-NAME>
							<PROVIDER-IREF>
								<CONTEXT-COMPONENT-REF
									DEST="SW-COMPONENT-PROTOTYPE">/TopLevel_Composite_SWC/Composite1_SWC_Inst</CONTEXT-COMPONENT-REF>
								<TARGET-P-PORT-REF DEST="P-PORT-PROTOTYPE">/Compsite1_SWC/Pp_SR_LightActuate_Composite</TARGET-P-PORT-REF>
							</PROVIDER-IREF>
							<REQUESTER-IREF>
								<CONTEXT-COMPONENT-REF
									DEST="SW-COMPONENT-PROTOTYPE">/TopLevel_Composite_SWC/LightActuate_SWC3</CONTEXT-COMPONENT-REF>
								<TARGET-R-PORT-REF DEST="R-PORT-PROTOTYPE">/LightActuate_SWC/Rp_SR_LightActuate</TARGET-R-PORT-REF>
							</REQUESTER-IREF>
						</ASSEMBLY-SW-CONNECTOR>
					</CONNECTORS>
				</COMPOSITION-SW-COMPONENT-TYPE>
			</ELEMENTS>
		</AR-PACKAGE>
	</AR-PACKAGES>
</AUTOSAR>